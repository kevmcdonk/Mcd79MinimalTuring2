{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "mcd79-minimal-turing": {
      "appLocation": "Mcd79MinimalTuringSite",
      "apiLocation": "Mcd79MinimalTuringSite\\api",
      "outputLocation": ".\\Mcd79MinimalTuringSite\\dist",
      "apiLanguage": "node",
      "apiVersion": "18",
      "appBuildCommand": "npm run build",
      "apiBuildCommand": "dotnet build /property:GenerateFullPaths=true /consoleloggerparameters:NoSummary",
      "run": "npm run dev",
      "appDevserverUrl": "http://localhost:4321"
    }
  }
}
